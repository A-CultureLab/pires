{
  "name": "back",
  "scripts": {
    "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/server.ts",
    "studio": "prisma studio",
    "build": "cross-env NODE_ENV=production npm -s run clean && tsc -b --force",
    "build:silent": "cross-env NODE_ENV=production npm -s run clean && npm run generate:prisma && tsc-silent -p tsconfig.json --suppress @/src",
    "start": " cross-env NODE_ENV=production node dist/server",
    "pm2": "cross-env NODE_ENV=production pm2-runtime start pm2.json",
    "clean": "rm -rf dist",
    "migrate": "prisma migrate dev",
    "generate": "npm -s run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/nexus"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.1.0",
    "@google-cloud/storage": "^5.8.5",
    "@prisma/client": "2.23.0",
    "apollo-server-express": "^2.25.2",
    "axios": "^0.21.1",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.10.0",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.10.0",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "0.35.0",
    "pm2": "^5.1.0",
    "prisma": "2.23.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/hpp": "^0.2.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.20.15",
    "@types/uuid": "^8.3.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "prisma-dbml-generator": "^0.6.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "tsc-silent": "^1.2.1",
    "typescript": "^4.3.5"
  }
}

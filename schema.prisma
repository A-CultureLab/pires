generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["orderByAggregateGroup"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  age       Int
  tests     Test2[]
}

model Test2 {
  id Int @id @default(autoincrement())

  userId Int

  user User @relation(fields: [userId], references: [id])
}
